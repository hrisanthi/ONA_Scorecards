schedule_json([{"event_type": "Other", "event_end": "25/9/14 19:00", "seats": "", "custom3": "", "session_ID": "Thursday-Registration", "artists": "", "speakers": "", "audience": "", "event_start": "25/9/14 7:30", "location": "Sheraton", "media_url": "", "rebelmouse": "", "description": "Head to the Sheraton Chicago Ballroom Promenade, near Chicago Ballroom VI, to pick up your badge. Have a question? Head to the help desk, near the main ballrooms. Or use the #ONA14help hashtag.", "track": "", "rsvp_url": "", "sponsors": "ScribbleLive, AFP", "hashtag": "#ONA14help", "active": "Y", "us_start": "9/25/14 7:30", "scribble": "", "name": "Registration + Help Desk", "host_org": "", "room": "Sheraton Chicago Ballroom Promenade", "session_format": "Registration", "exhibitors": "", "moderators": "", "us_end": "9/25/14 19:00", "geo_area": "", "volunteers": ""}, {"event_type": "Solve", "event_end": "25/9/14 9:00", "seats": "", "custom3": "", "session_ID": "1st-Timers", "artists": "", "speakers": "Jessica Estepa", "audience": "General", "event_start": "25/9/14 8:30", "location": "Sheraton", "media_url": "", "rebelmouse": "", "description": "Never been to an ONA conference? This session is just for you. How do you connect with conference veterans, and at what events? How do you choose between an inspiring talk and that nuts-and-bolts training you had your eye on? And what can you expect from the tech toys in the Midway? We'll make sure you're equipped to get the most out of your ONA experience. Don't forget to bring your ONA #1 button - your first-timer badge of honor.", "track": "", "rsvp_url": "", "sponsors": "", "hashtag": "#1stONA", "active": "Y", "us_start": "9/25/14 8:30", "scribble": "", "name": "You Are #1! ONA14 First-Timer Orientation", "host_org": "Online News Association", "room": "Chicago Ballrooms", "session_format": "Interactive Discussion", "exhibitors": "", "moderators": "", "us_end": "9/25/14 9:00", "geo_area": "", "volunteers": ""}, {"event_type": "Listen", "eve